{
  "name": "@moonlight-io/asteroid-sdk-js",
  "version": "0.10.0",
  "description": "",
  "main": "dist/index",
  "types": "dist/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moonlight-io/asteroid-sdk-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint",
    "prettier": "prettier",
    "clean": "rm -rf ./dist",
    "dev": "npm run clean && tsc --watch",
    "build": "npm run clean && tsc",
    "rebuild": "rm -rf ./node_modules && npm install",
    "rebuild:hard": "rm -rf package-lock.json && npm run rebuild",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json './src/**/*.ts' --fix",
    "format": "prettier --write './{src,examples,tests,data}/**/*.{ts,js,json}'",
    "format:windows": "prettier --write \"./{src,examples,tests,data}/**/*.{ts,js,json}\"",
    "test": "jest ./tests/**/*",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "dependencies": {
    "@cityofzion/neon-js": "^4.7.1",
    "@types/lodash": "^4.14.149",
    "axios": "^0.19.0",
    "build-url": "^2.0.0",
    "crypto": "^1.0.1",
    "elliptic": "^6.5.2",
    "json-rpc-error": "^2.0.0",
    "lodash": "^4.17.15",
    "node-log-it": "^2.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
